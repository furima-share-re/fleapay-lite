    "next": "^14.2.0",
    "openai": "^4.28.4",
    "pg": "^8.11.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "sharp": "^0.33.2",
