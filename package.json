    "start": "node .next/standalone/server.js",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "node scripts/copy-static-files.js",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "pre-commit-check": "npm run type-check && npm run lint",
    "check-build-errors": "node scripts/check-build-errors.js",
